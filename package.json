{
    "name": "ngx-guided-tour-demo",
    "version": "0.0.0",
    "resolutions": {
        "normalize-url": "6.1.0",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1"
    },
    "scripts": {
        "preinstall": "npx npm-force-resolutions",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:all": "ng build && npm run build:lib",
        "build:lib": "ng build ngx-guided-tour && npm run build:lib:scss && npm run build:lib:css",
        "build:lib:css": "node-sass ./projects/ngx-guided-tour/src/lib/guided-tour-base-theme.scss -o ./dist/ngx-guided-tour/css/ && node-sass ./projects/ngx-guided-tour/src/lib/guided-tour.component.scss -o ./dist/ngx-guided-tour/css/",
        "build:lib:scss": "mkdir ./dist/ngx-guided-tour/scss/ && cp ./projects/ngx-guided-tour/src/lib/guided-tour-base-theme.scss ./dist/ngx-guided-tour/scss/guided-tour-base-theme.scss && cp ./projects/ngx-guided-tour/src/lib/guided-tour.component.scss ./dist/ngx-guided-tour/scss/guided-tour.component.scss",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "publish": "cp README.md dist/ngx-guided-tour/README.md && cd dist/ngx-guided-tour && npm publish"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "bootstrap": "4.3.1",
        "core-js": "2.6.6",
        "node-sass": "4.13.1",
        "rxjs": "6.3.3",
        "tslib": "1.9.3",
        "zone.js": "0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.25",
        "@angular-devkit/build-ng-packagr": "0.803.25",
        "@angular/cli": "8.2.2",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.14",
        "@types/jasmine": "2.8.16",
        "@types/jasminewd2": "2.0.6",
        "@types/node": "8.9.5",
        "codelyzer": "4.5.0",
        "jasmine-core": "2.99.1",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "6.3.4",
        "normalize-url": "6.1.0",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.5",
        "karma-jasmine": "1.1.2",
        "karma-jasmine-html-reporter": "0.2.2",
        "ng-packagr": "4.7.1",
        "protractor": "5.4.2",
        "ts-node": "7.0.1",
        "tsickle": "0.35.0",
        "tslib": "1.9.3",
        "tslint": "5.11.0",
        "typescript": "3.5.3"
    }
}
