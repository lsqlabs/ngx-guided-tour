{
    "name": "ngx-guided-tour-demo",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:all": "ng build && npm run build:lib",
        "build:lib": "ng-packagr -p projects/ngx-guided-tour/package.json && npm run build:lib:css && npm run build:lib:scss",
        "build:lib:css": "sass ./projects/ngx-guided-tour/src/lib/guided-tour-base-theme.scss ./dist/ngx-guided-tour/css/guided-tour-base-theme.css && sass ./projects/ngx-guided-tour/src/lib/guided-tour.component.scss ./dist/ngx-guided-tour/css/guided-tour.component.css",
        "build:lib:scss": "mkdir ./dist/ngx-guided-tour/scss/ && cp ./projects/ngx-guided-tour/src/lib/guided-tour-base-theme.scss ./dist/ngx-guided-tour/scss/guided-tour-base-theme.scss && cp ./projects/ngx-guided-tour/src/lib/guided-tour.component.scss ./dist/ngx-guided-tour/scss/guided-tour.component.scss",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "publish": "cp README.md dist/ngx-guided-tour/README.md && cd dist/ngx-guided-tour && npm publish"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.0.0",
        "@angular/common": "^13.0.0",
        "@angular/compiler": "^13.0.0",
        "@angular/core": "^13.0.0",
        "@angular/forms": "^13.0.0",
        "@angular/platform-browser": "^13.0.0",
        "@angular/platform-browser-dynamic": "^13.0.0",
        "@angular/router": "^13.0.0",
        "bootstrap": "4.6.1",
        "core-js": "^3.21.1",
        "rxjs": "^6.6.7",
        "zone.js": "0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.3.0",
        "@angular/cli": "^13.0.0",
        "@angular/compiler-cli": "^13.0.0",
        "@angular/language-service": "^13.0.0",
        "@types/jasmine": "2.8.16",
        "@types/jasminewd2": "2.0.6",
        "@types/node": "^16.0.0",
        "codelyzer": "6.0.2",
        "jasmine-core": "4.0.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.3.17",
        "karma-chrome-launcher": "3.1.1",
        "karma-coverage-istanbul-reporter": "2.0.5",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ng-packagr": "^13.3.0",
        "normalize-url": "6.1.0",
        "protractor": "7.0.0",
        "sass": "^1.49.9",
        "serialize-javascript": "6.0.0",
        "trim-newlines": "3.0.1",
        "ts-node": "10.7.0",
        "tslib": "^2.3.0",
        "tslint": "5.11.0",
        "typescript": "^4.6.0"
    }
}
